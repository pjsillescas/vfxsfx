#version 330 core

in vec3 colorFragment;
in vec2 texFragment;

out vec4 out_color;

uniform sampler2D textura;
uniform sampler2D textura2;
uniform float mixtextures;

void main() {

	out_color = mix( texture(textura,texFragment), texture(textura2,texFragment), mixtextures) * vec4(colorFragment.rgb, 1.0);

}
